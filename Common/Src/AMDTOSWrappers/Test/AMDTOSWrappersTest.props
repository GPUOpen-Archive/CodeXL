<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <CommonDir>..\..\..\Common\</CommonDir>
    <AMDTOutputDir>..\..\..\CodeXL\Output\</AMDTOutputDir>
    <CommonProjectsDir>..\..\..\CommonProjects\</CommonProjectsDir>
    <GDT_PUBLIC />
  </PropertyGroup>
  <PropertyGroup>
    <OutDir>$(AMDTOutputDir)$(Configuration)\bin\</OutDir>
    <IntDir>$(AMDTOutputDir)$(Configuration)\obj\$(ProjectName)\$(Platform)$(GDTBuildSuffix)\</IntDir>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <Link />
    <ClCompile />
    <Link>
      <AdditionalLibraryDirectories>$(AMDTOutputDir)$(Configuration)\arch</AdditionalLibraryDirectories>
      <AdditionalDependencies>AMDTBaseTools$(GDTProjectSuffix).lib;AMDTOSWrappers$(GDTProjectSuffix).lib;kernel32.lib;user32.lib;gdi32.lib;winspool.lib;comdlg32.lib;advapi32.lib;shell32.lib;ole32.lib;oleaut32.lib;uuid.lib;odbc32.lib;odbccp32.lib;%(AdditionalDependencies)</AdditionalDependencies>
    </Link>
    <ClCompile>
      <AdditionalIncludeDirectories>$(CommonProjectsDir);%(AdditionalIncludeDirectories)</AdditionalIncludeDirectories>
      <RuntimeLibrary>MultiThreadedDLL</RuntimeLibrary>
    </ClCompile>
  </ItemDefinitionGroup>
  <ItemGroup>
    <BuildMacro Include="CommonDir">
      <Value>$(CommonDir)</Value>
    </BuildMacro>
    <BuildMacro Include="AMDTOutputDir">
      <Value>$(AMDTOutputDir)</Value>
    </BuildMacro>
    <BuildMacro Include="CommonProjectsDir">
      <Value>$(CommonProjectsDir)</Value>
    </BuildMacro>
    <BuildMacro Include="GDT_PUBLIC">
      <Value>$(GDT_PUBLIC)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>