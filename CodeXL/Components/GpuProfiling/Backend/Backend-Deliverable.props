<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup Label="UserMacros">
    <CodeXLGpuProfilerDeliverDir>$(CodeXLGpuProfilingDir)\Build\CodeXLGpuProfiler\$(Configuration)-$(Platform)$(AMDTBuildSuffix)</CodeXLGpuProfilerDeliverDir>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <PostBuildEvent>
      <Command>xcopy /r /d /y /c "$(TargetPath)" "$(CodeXLGpuProfilerDeliverDir)\"</Command>
    </PostBuildEvent>
  </ItemDefinitionGroup>
  <ItemGroup />
</Project>
