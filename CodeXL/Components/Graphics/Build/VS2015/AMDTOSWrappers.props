<?xml version="1.0" encoding="utf-8"?>
<Project DefaultTargets="Build" ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup Label="UserMacros">
    <!--CustomPreProcessorDefs-->
    <CustomPreProcessorDefs Condition="'$(SolutionName)'=='CodeXL-AllProjects'">AMDTOSWRAPPERS_STATIC;AMDTBASETOOLS_STATIC;CODEXL_GRAPHICS</CustomPreProcessorDefs>
    <CustomPreProcessorDefs Condition="'$(SolutionName)'!='CodeXL-AllProjects'">AMDTOSWRAPPERS_STATIC;AMDTBASETOOLS_STATIC</CustomPreProcessorDefs>
    <CommonProjectsDir>$(CommonDir)\Src</CommonProjectsDir>
  </PropertyGroup>
  <PropertyGroup>
    <_ProjectFileVersion>10.0.30319.1</_ProjectFileVersion>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <ClCompile>
      <AdditionalIncludeDirectories>$(CommonProjectsDir);%(AdditionalIncludeDirectories)</AdditionalIncludeDirectories>
      <PreprocessorDefinitions>$(CustomPreProcessorDefs);%(PreprocessorDefinitions)</PreprocessorDefinitions>
    </ClCompile>
    <Lib>
      <AdditionalDependencies>%(AdditionalDependencies)</AdditionalDependencies>
      <AdditionalLibraryDirectories>%(AdditionalLibraryDirectories)</AdditionalLibraryDirectories>
    </Lib>
    <ResourceCompile>
      <PreProcessorDefinitions Condition="'$(SolutionName)'=='CodeXL-AllProjects'">%(PreprocessorDefinitions);CODEXL_GRAPHICS</PreProcessorDefinitions>
    </ResourceCompile>
  </ItemDefinitionGroup>
  <ItemGroup />
</Project>
