<?xml version="1.0" encoding="utf-8"?>
<Project DefaultTargets="Build" ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup Label="UserMacros">
    <!-- This is only to avoid failing the build on 32-bit Windows. When HSA support for Windows exists, we'll re-examine this. -->
    <HSAPlatform Condition="'$(GDTPlatform)' == 'x86'">x86_64</HSAPlatform>
  </PropertyGroup>
  <ImportGroup Label="HwDbgProjects">
    <Import Project="$(AMDTCommonAMD)\HwDebugger\Global-HwDbgHSA.props" />
    <Import Project="$(AMDTCommonAMD)\HwDebugger\Global-HwDbgFacilities.props" />
    <Import Project="$(CommonDir)\Lib\AMD\HSA\Global-HSAFoundation-NoLib.props" />
  </ImportGroup>
  <ItemDefinitionGroup>
    <Link>
<!--      <AdditionalDependencies>AMDGPUDebugHSA$(GDTPlatformSuffix)$(GDTDebugSuffix).lib;%(AdditionalDependencies)</AdditionalDependencies> -->
    </Link>
  </ItemDefinitionGroup>
</Project>